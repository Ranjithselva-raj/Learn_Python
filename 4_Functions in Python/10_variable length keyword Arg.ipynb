    "# variable length keyword argument in function always comes **kwargs\n",
    "\n",
    "def fun(**kwargs):\n",
    "    for key,value in kwargs.items():\n",
    "\n",
    "        print(f\"{key} : {value}\")\n",
    "\n",
    "\n",
    "fun(product=\"shoes\", price= 1000)\n",
